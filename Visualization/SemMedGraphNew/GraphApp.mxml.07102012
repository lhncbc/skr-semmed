			private function centerOnVisible():void {
				//if (!firstTime)
				//	return;
				if (seed.node && seed.node.visible) {
					var n:NodeSprite = seed.node as NodeSprite;
					trace("I'm centering on root");
					moveToCenter(n.parent.x + n.x, n.parent.y + n.y, true);
				}else
